<!-- MyBatis  DataSource에 대한 설정 및  SQL 정의 xml 에 대한 매핑-->
<configuration>	
	<!-- 사용할 클래스에 대한 별칭을 명시한다.  -->
	<typeAliases>		
		<package name="org.asechs.wheelwego.model"/>		
	</typeAliases>	
    <!-- mybatis sql 정의 xml을 mapping  -->
	<mappers>
		<mapper resource="mybatis/config/member.xml"></mapper>	
	</mappers>
</configuration>










